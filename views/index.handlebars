<nav class="navbar">
    <div class="container-fluid">
        <div class="navbar-header">
            <a href="/" class="navbar-brand">Mongo Scraper: Articles from Wall Street Journal</a>
            <ul class="nav navbar-nav">
                <li class="active"><a href="/">Home</a></li>
                <li><a href="/save">Saved Articles</a></li>
                <li><a class="btn scrape-new">Scrape New Articles</a></li>
            </ul>
        </div>
    </div>
</nav>

<div class="jumbotron">
    <h1>Mongo Scraper</h1>
    <p>Wall Street Journal Edition</p>
</div>

<div class="container-fluid">
    <div class="container" id="div-container">
        <p>WSJ Scraper</p>
        {{#each data}}
        <div class="card">
            <h3 class="card-header"><a href="{{link}}">{{this.title}}</a></h3>
            <p class="card-text">{{this.summary}}</p>
            <a href="#" data-id="{{_id}}" id="save" class="btn">Save</a>
            <a href="{{link}}" type="button" class="btn">Read More</a>
        </div>
        {{/each}}

    </div>
</div> {{!-- this ends the container --}}

{{!-- JAVASCRIPT --}}
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js"></script>
<script src="js/index.js" type="text/javascript"></script>